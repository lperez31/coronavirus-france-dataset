    //Fonction carrousel article avec modal : creation du slider dans la modal
    function doArticleSliderModal(indexImg) {
        var pageArticleSlideShowList = $('#myModal .page-article--slide-show--modal');
        if (pageArticleSlideShowList.length) {
            var settings = {
                accessibility: true,
                slidesToShow: 1,
                slidesToScroll: 1,
                arrows: true,
                dots: false,
                initialSlide: indexImg,
                responsive: [
                    {
                        breakpoint: 767,
                        settings: {
                            arrows: false,
                            dots: true,
                            slidesToShow: 1,
                            slidesToScroll: 1
                        }
                    }
                ]
            };
            pageArticleSlideShowList.slick(settings);
        }
    }

    //Fonction carrousel article avec modal : recuperation du html
    function articleCarrouselModal(){
        // Get the modal
        var modal = $('#myModal');

        // Get the image and insert it inside the modal - use its "alt" text as a caption
        var img = $("#myImg img");
        var modalImg = $("#img01");
        var captionText = $("#caption");
        var html = $('#myImg + div');
        img.each(function(element){
            $(this).on('click', function(){
                //Index image on click
                var indexImg = $(this).closest('.page-article--slide-show--item').index() - 1;
                //Add overflow hidden on body
                $("body").addClass('body-overflow');
                var htmlSlideshow = $(this).closest('.page-article--slide-show').find('.page-article--slide-show--modal').html();
                modal.show();
                $("#carrousel-modal").html(htmlSlideshow).wrapInner('<div class="page-article--slide-show--modal" />');
                modal.find('.page-article--slide-show--modal').show();
                doArticleSliderModal(indexImg);
            });
        });

        // When the user clicks on <span> (x), close the modal
        $(".close").on('click',function() {
            modal.hide();
            $("body").removeClass('body-overflow');
        });
    }

    //COUCHE A LA UNE NATIONALE
    function switchActusCoucheALaUne() {
        $('.accueil-a-la-une--contenu').find('li button').each(function () {
            $(this).on('click',function () {
                var ariaControls = $(this).attr('aria-controls');
                $('.accueil-a-la-une--contenu .tab-pane').removeClass('active');
                $('.accueil-a-la-une--contenu .tab-pane#'+ariaControls).addClass('active');
            });

        });

        if ($('.accueil-a-la-une--contenu').find('li button').length == 1){
            $('.accueil-a-la-une--contenu').find('li').addClass('active');
            $('.accueil-a-la-une--contenu').find('li button').addClass('actu-alone');
            $('.accueil-a-la-une--contenu').find('.actu-alone').off('click');
        }
    }

    /**
     * arsFormHandler
     * Gère les input et label dans un formulaire
     * avec la class @see config.customClass
     */
    var arsFormHandler = function (param) {
        'use strict';

        // Configuration par défaut
        var config = {
            formClass: 'form-ars-custom',
            classes: {
                focus: 'focused',
                filled: 'filled',
                bind: 'ars-form-js-handler-bind'
            }
        };
        config = typeof param !== 'undefined' ? param : config;

        config.form = $('.' + config.formClass);

        if (config.form.length) {
            config.inputs = config.form.find('input,textarea').not('[type="checkbox"]');

            /**
             * Fonction permettant de trouver le parent sur
             * lequel mettre les class
             * @param elem
             * @returns jQuery Object
             */
            var findFormGroup = function (elem) {
                return elem.parents('.form-group:not(.form-inline)');
            };

            /**
             * Fonction permettant de tester le contenu de l'input
             * @param elem
             */
            var filledCheck = function (elem) {
                var formGroupElem = findFormGroup(elem);

                var elemFilled = ((elem.val() !== '') || ((elem.attr('placeholder') !== '') && (elem.attr('placeholder'))));

                if (elemFilled && formGroupElem.length && !($(formGroupElem[0]).hasClass(config.classes.filled))) {
                    $(formGroupElem[0]).addClass(config.classes.filled);
                } else if (!elemFilled) {
                    formGroupElem.removeClass(config.classes.filled);
                }
            };

            /**
             * Parcourir les inputs
             */
            config.inputs.each(function () {
                if (!($(this).hasClass(config.classes.bind))) {
                    $(this).addClass(config.classes.bind);

                    $(this).on('focus', function () {
                        var formGroupElem = findFormGroup($(this));
                        if (formGroupElem.length && !($(formGroupElem[0]).hasClass(config.classes.focus))) {
                            $(formGroupElem[0]).addClass(config.classes.focus);
                        }
                    }).on('blur', function () {
                        var formGroupElem = findFormGroup($(this));
                        formGroupElem.removeClass(config.classes.focus);
                    }).on('change', function () {
                        filledCheck($(this));
                    });

                    // Executer un pre check sur les champs déjà remplis
                    filledCheck($(this));
                }
            });

        }
    };

    var deleteElementByHeight = function (param) {
        'use strict';

        // Configuration par défaut
        var config = {
            minHeight: 50, // Hauteur minimal pour qu'un block s'affiche
            elementsToTest: null
        };
        config = typeof param !== 'undefined' ? param : config;

        $.each(config.elementsToTest, function () {
            if (this.offsetHeight < config.minHeight) {
                this.parentElement.removeChild(this); // Suppression d'un block presume vide
            }
        });
    };

    // Fonction Switch de vue annuaire : Liste / Map
    function switchVueAnnuaire() {
        if ($('.view-annuaire').length > 0){

            $('.page-liste--switch-vue--carte').on('click', function (e){
                e.preventDefault();
                $('.page-liste--list').hide();
                $('.pager .button').hide();
                $('.button-export').hide();
                $('#map_annuaire').show();
                $(this).addClass('active');
                $('.page-liste--switch-vue--liste').removeClass('active');
            });

            $('.page-liste--switch-vue--liste').on('click', function (e){
                e.preventDefault();
                $('#map_annuaire').hide();
                $('.page-liste--list').show();
                $('.pager .button').show();
                $('.button-export').show();
                $(this).addClass('active');
                $('.page-liste--switch-vue--carte').removeClass('active');
            });
        }
    }

    // Fonction carrousel filtres annuaire
    function annuaireSlider() {
        if ($('.view-annuaire').length > 0){
            var popLists = $('.page-liste--formulaire .pop-list');
            if (popLists.length) {
                popLists.slick({
                    slidesToShow: 4,
                    slidesToScroll: 1,
                    arrows: true,
                    dots: false,
                    //adaptiveHeight: true,
                    responsive: [
                        {
                            breakpoint: 991,
                            settings: {
                                arrows: true,
                                slidesToShow: 3,
                                slidesToScroll: 3
                            }
                        },
                        {
                            breakpoint: 767,
                            settings: {
                                arrows: true,
                                slidesToShow: 1,
                                slidesToScroll: 1
                            }
                        }
                    ]
                });
            }

        }
    }

    /* Relancer fonction annuaire au clic sur les catégories annuaire */
    $(document).ajaxComplete(function(event,xhr,settings){
        var chaine="/formulaire/suggerer-un-evenement";
        var url=settings.url;
        if(url.substr(0,chaine.length) ==chaine)
            hideButtonSuggerer();

        if(settings.url === "/views/ajax?_wrapper_format=drupal_ajax" || settings.url.substring(0,51) ==='/views/ajax?field_categorie_annuaire_ars_target_id=' ) {
            annuaireSlider();
            hideCategoryAllAnnuaire();
            switchVueAnnuaire();
        }
        if(($('.page-liste--switch-vue--carte').hasClass('active'))){
            $('.pager .button').hide();
        }

        var formulaireSuggerrerEvent = /\/formulaire\/suggerer-un-evenement/;
        if(settings.url.match(formulaireSuggerrerEvent)) {
            arsFormHandler();
            deleteElementByHeight({
                minHeight: 50, // Hauteur minimal pour qu'un block s'affiche
                elementsToTest: $('.field-multiple-table').find('.draggable') // Listes d'élements à checker
            });
        }
    });

    // Cacher select categories annuaire si pas de categories
    function hideCategoryAllAnnuaire(){
        $('.path-annuaire .page-liste--formulaire .category-all').parent().hide();
    }

    // Fonction print des pages intérieures
    function printPagesInterieures() {
        $('.page-article--outils--print').off().on('click',function (e) {
            e.preventDefault();
            window.print();
        });
    }


    //Fonction formulaire Suggérer un évènement
    function formEventSuggest() {
        if($('.form-event-suggest').length > 0){
            $('.field--name-field-doc-telecharge-ars .field-add-more-submit').trigger('mousedown');
            $('.field--name-field-body-ars .field-add-more-submit').trigger('mousedown');

        }
    }

    var cookiePopupHandler = function (param) {
        'use strict';

        // Configuration par défaut
        var config = {
            popupID: 'sliding-popup'
        };
        config = typeof param !== 'undefined' ? param : config;

        config.popup = $('#' + config.popupID);

        if (config.popup.length) {
            config.popup.addClass('hidden-print');
        }
    };

    function suggestEventDatePicker(){
        if(window.location.href.indexOf("/formulaire/suggerer-un-evenement")>-1) {
            var isChrome = !!window.chrome && !!window.chrome.webstore;
            if (isChrome) {
                $('input#edit-field-date-evenement-0-value-date').datepicker({
                    dateFormat: 'yy-mm-dd',
                    altField: '#thealtdate',
                    altFormat: 'yy-mm-dd'
                });
                $('input#edit-field-date-evenement-0-value-date').click(function (e) {
                    e.preventDefault();
                });
                $('input#edit-field-date-evenement-0-end-value-date').datepicker({
                    dateFormat: 'yy-mm-dd',
                    altField: '#thealtdate',
                    altFormat: 'yy-mm-dd'
                });
                $('input#edit-field-date-evenement-0-end-value-date').click(function (e) {
                    e.preventDefault();
                });
            }
        }
    }

    //VERIFICATION SI EN MODE CONTENU RESTREINT
    function checkModeContenuRestreint() {
        if(!$('header').find('.identification-site').next().length){
            $('body').addClass('restricted-content');
        }
    }

    //CARROUSEL THEMATIQUE : HIDE BOUTTON PAUSE SI UNE SEULE IMAGE
    function carrouselThematiqueBtnPause() {
        if ($('.body__ars_theme--arstheme_thematique').length) {
            if($('#slider-simple').find('.slick-slide').length == 1) {
                $('.carrousel-pause-thematique').hide();
            }
        }
    }

    function requeteRechercheAjax(){
        if(isMobile){
            $("button#edit-submit-recherche-globale").click(function() {
                var search = $("input#edit-search-ars")[0].value;
                var req = "/recherche-globale?search_ars="+search;
                var filtre = $('[type="checkbox"]:checked');
                for(i=0;i<filtre.length;i++){
                    req += "&"+filtre[i].name+"="+filtre[i].value;
                }
                var date_start = $('#edit-field-date-publication-ars')[0].value;
                var date_end = $('#edit-field-date-publication-ars-end')[0].value;
                if(date_start) {
                    req += "&field_date_publication_ars=" + date_start;
                }
                if(date_end) {
                    req += "&field_date_publication_ars_end=" + date_end;
                }
                window.location = req;
            });
        }
    }

    function modificationhrefmailtoshare(){
        var site = document.title.split("| ")[1];
        var share = $("a.email.share");
        if(share[0]) {
            var s = share[0].href;
            share[0].href = s.replace("[ars_site:name]", site).replace("&%23039;", "'").replace("&#039;", "'");
            $('a.disabled').removeClass("disabled");
        }
    }
    /*function emulateLinkClick(link) {
        var form = $("<form></form>");

        form.attr({
            id : "formform",
            action : $(link).attr("href"),
            method : "GET"
        });

        $("body").append(form);
        $("#formform").submit();
        $("#formform").remove();
    }*/

    function blocAllClickable(){
        var $blocs = $('.accueil-a-la-une .accueil-a-la-une--article, .accueil-a-la-une .accueil-a-la-une--agenda, .accueil-appels-projets--item');
        if ($blocs.length) {
            $blocs.each(function() {
                $(this).off('click').on('click', function (event) {
                    if (!$(event.target).is('a')) {
                        $(this).find('a')[0].click();
                    }
              });
            });
        }
    }

    // Initialisation de toutes les fonctions
    function initArs (eventType) {
        "use strick";
        isMobile = $(window).width() < 992;
        isMobileMenu = $(window).width() < 1200;

        cookiePopupHandler();
        doStickyMegaMenu();
        doMegaMenu();
        telecommande();
        if (eventType!="resize") {
            doSearchButtonRwd();
        }
        filtresListes();
        doFooterSelectSite();
        doRegionSelect();
        doTopOfPage();
        sommaireWithStickyMenu();
        arsFormHandler();
        blocAllClickable();
        if (eventType==="resize") {
            var accueilCarrouselAlternative = $('.accueil-carrousel-alternative');
            var pageArticleSlideShowList = $('.page-article--slide-show--list');
            var accueilRecherchePleineVisuel = $('.accueil-recherche-pleine--visuel');
            var accueilVoirAussiList = $('.article--voir-aussi--list');
            var evenementVoirAussiList = $('.evenement--voir-aussi--list');

            if (isMobileMenu && $('.mega-menu--inner .mega-menu--item[role="search"]').length) {
                $('.region-header').append($('.mega-menu--inner .mega-menu--item[role="search"]'));
            }
            if (!isMobileMenu && $('.region-header>.mega-menu--item[role="search"]').length) {
                $('.mega-menu--inner .mega-menu--list').append($('.region-header>.mega-menu--item[role="search"]'));
            }
            if ($('.accueil-partenaires').length) {
                $('.accueil-partenaires--list').slick("destroy");
            }
            if ($('.accueil-services').length) {
                //$('.accueil-services--liste').slick('resize');
                $('.accueil-services--liste').slick("destroy");
            }
            if (accueilCarrouselAlternative.length) {
                $('.accueil-carrousel-actus--list>div').slick("destroy");
                $('.accueil-carrousel-alternative--list>div').slick("destroy");
            }
            if (pageArticleSlideShowList.length) {
                pageArticleSlideShowList.slick("destroy");
            }
            if (accueilRecherchePleineVisuel.length) {
                accueilRecherchePleineVisuel.slick("destroy");
            }
            if (accueilVoirAussiList.length) {
                accueilVoirAussiList.slick("destroy");
            }
            if (evenementVoirAussiList.length) {
                evenementVoirAussiList.slick("destroy");
            }

        }
        carrouselSlick();
        articleCarrouselModal();
        doArticleSlider();
        doHomeSearchSlider();
        doArticleVoirAussiSlider();
        doEtudePublicationSlider();
        doEvenementVoirAussiSlider();
        switchActusCoucheALaUne();
        printPagesInterieures();
        switchVueAnnuaire();
        annuaireSlider();
        hideCategoryAllAnnuaire();
        suggestEventDatePicker();
        checkModeContenuRestreint();
        carrouselThematiqueBtnPause();
        requeteRechercheAjax();
        modificationhrefmailtoshare();
    }

    // Global var
    var isMobile;
    var isMobileMenu;
    var widthRef;
    var widthWindow;

    $(window).on("load", function() {
        widthWindow = $(window).width();
        initArs();
    });
    $(window).on("resize", function() {
        var widthResize = $(window).width();
        console.log('widthResize :'+widthResize);
        if(widthWindow != widthResize){
            initArs("resize");
        }
    });
    window.addEventListener("orientationchange", function() {
        widthWindow = $(window).width();
        var widthResize = $(window).width();
        if(widthWindow != widthResize) {
            initArs("resize");
        }
    });

    $('.bef-datepicker').click(function (e) {
        $('.dropdown-menu').removeClass('in');
        $('.dropdown-menu').attr('aria-expanded', false);
    });

    $(document).on("click", "button[type=submit]", function(evt){
        var target = evt.currentTarget;
        var name = target.name;
        var value = target.value;
        $(target).before($("<input type='hidden' name='" + name + "' value='" + value + "' />"));
    });

})(jQuery, Drupal, this, this.document);
;

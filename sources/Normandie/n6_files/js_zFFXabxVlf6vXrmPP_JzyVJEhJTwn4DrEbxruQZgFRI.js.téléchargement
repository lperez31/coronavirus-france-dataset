rt(Math.pow(b.touchObject.curY-b.touchObject.startY,2)))),e=b.swipeDirection(),"vertical"!==e?(void 0!==a.originalEvent&&b.touchObject.swipeLength>4&&a.preventDefault(),g=(b.options.rtl===!1?1:-1)*(b.touchObject.curX>b.touchObject.startX?1:-1),b.options.verticalSwiping===!0&&(g=b.touchObject.curY>b.touchObject.startY?1:-1),f=b.touchObject.swipeLength,b.touchObject.edgeHit=!1,b.options.infinite===!1&&(0===b.currentSlide&&"right"===e||b.currentSlide>=b.getDotCount()&&"left"===e)&&(f=b.touchObject.swipeLength*b.options.edgeFriction,b.touchObject.edgeHit=!0),b.options.vertical===!1?b.swipeLeft=d+f*g:b.swipeLeft=d+f*(b.$list.height()/b.listWidth)*g,b.options.verticalSwiping===!0&&(b.swipeLeft=d+f*g),b.options.fade===!0||b.options.touchMove===!1?!1:b.animating===!0?(b.swipeLeft=null,!1):void b.setCSS(b.swipeLeft)):void 0)},b.prototype.swipeStart=function(a){var c,b=this;return b.interrupted=!0,1!==b.touchObject.fingerCount||b.slideCount<=b.options.slidesToShow?(b.touchObject={},!1):(void 0!==a.originalEvent&&void 0!==a.originalEvent.touches&&(c=a.originalEvent.touches[0]),b.touchObject.startX=b.touchObject.curX=void 0!==c?c.pageX:a.clientX,b.touchObject.startY=b.touchObject.curY=void 0!==c?c.pageY:a.clientY,void(b.dragging=!0))},b.prototype.unfilterSlides=b.prototype.slickUnfilter=function(){var a=this;null!==a.$slidesCache&&(a.unload(),a.$slideTrack.children(this.options.slide).detach(),a.$slidesCache.appendTo(a.$slideTrack),a.reinit())},b.prototype.unload=function(){var b=this;a(".slick-cloned",b.$slider).remove(),b.$dots&&b.$dots.remove(),b.$prevArrow&&b.htmlExpr.test(b.options.prevArrow)&&b.$prevArrow.remove(),b.$nextArrow&&b.htmlExpr.test(b.options.nextArrow)&&b.$nextArrow.remove(),b.$slides.removeClass("slick-slide slick-active slick-visible slick-current").attr("aria-hidden","true").css("width","")},b.prototype.unslick=function(a){var b=this;b.$slider.trigger("unslick",[b,a]),b.destroy()},b.prototype.updateArrows=function(){var b,a=this;b=Math.floor(a.options.slidesToShow/2),a.options.arrows===!0&&a.slideCount>a.options.slidesToShow&&!a.options.infinite&&(a.$prevArrow.removeClass("slick-disabled").attr("aria-disabled","false"),a.$nextArrow.removeClass("slick-disabled").attr("aria-disabled","false"),0===a.currentSlide?(a.$prevArrow.addClass("slick-disabled").attr("aria-disabled","true"),a.$nextArrow.removeClass("slick-disabled").attr("aria-disabled","false")):a.currentSlide>=a.slideCount-a.options.slidesToShow&&a.options.centerMode===!1?(a.$nextArrow.addClass("slick-disabled").attr("aria-disabled","true"),a.$prevArrow.removeClass("slick-disabled").attr("aria-disabled","false")):a.currentSlide>=a.slideCount-1&&a.options.centerMode===!0&&(a.$nextArrow.addClass("slick-disabled").attr("aria-disabled","true"),a.$prevArrow.removeClass("slick-disabled").attr("aria-disabled","false")))},b.prototype.updateDots=function(){var a=this;null!==a.$dots&&(a.$dots.find("li").removeClass("slick-active").attr("aria-hidden","true"),a.$dots.find("li").eq(Math.floor(a.currentSlide/a.options.slidesToScroll)).addClass("slick-active").attr("aria-hidden","false"))},b.prototype.visibility=function(){var a=this;a.options.autoplay&&(document[a.hidden]?a.interrupted=!0:a.interrupted=!1)},a.fn.slick=function(){var f,g,a=this,c=arguments[0],d=Array.prototype.slice.call(arguments,1),e=a.length;for(f=0;e>f;f++)if("object"==typeof c||"undefined"==typeof c?a[f].slick=new b(a[f],c):g=a[f].slick[c].apply(a[f].slick,d),"undefined"!=typeof g)return g;return a}});;
/**
 * @file A11Y for slick
 * @author Faisal HUSSAIN <faisal.hussain@kleegroup.com>
 */

// ToDo: a11y
(function($, Drupal, window, document, undefined) {
    'use strict';

    var $el = document.createElement("div");
    $el.setAttribute("class", "monPseudoElementSlick-20170321")
    var $elChild = document.createElement("li");
    $el.appendChild($elChild);
    $elChild = document.createElement("li");
    $el.appendChild($elChild);
    $elChild = document.createElement("li");
    $el.appendChild($elChild);
    document.body.appendChild($el);

    $(window).on("load", function () {
        var $el = $('div.monPseudoElementSlick-20170321').slick();
        var slick = $el[0].slick;

        var Slick = slick.constructor;
        var destroy = Slick.prototype.destroy;
        var initADA = Slick.prototype.initADA;
        var buildArrows = Slick.prototype.buildArrows;

        Slick.prototype.initADA = function() { // https://github.com/kenwheeler/slick/issues/2706
            // Slick default
            initADA.apply(this, arguments);

            var _ = this;

            _.$slideTrack.removeAttr('role'); // suppression du role listbox

            _.$slides.not(_.$slideTrack.find('.slick-cloned')).each(function(i) { // modification du role option en => role tabpanel
                $(this).attr({
                    'role': 'tabpanel',
                    'id': 'navigation' + _.instanceUid + i + ''
                });
            });
            if (_.$dots !== null) { // modification du role navigation en => role tab
                _.$dots.attr('role', 'tablist').find('li').each(function() {
                    $(this).attr({
                        'role': 'tab'
                    });
                });
            }
            if (!(_.$slider.hasClass('slick-slider'))) { // fix destroy prototype
                _.$slides.find('a, input, button, select').removeAttr('tabindex').end()
                    .removeAttr('aria-describedby role aria-hidden tabindex');
            }
        };
        Slick.prototype.destroy = function(refresh) {
            // Slick default
            destroy.apply(this, arguments);

            var _ = this;

            if (!(_.$slider.hasClass('slick-slider'))) { // fix destroy prototype
                _.$slides.find('a, input, button, select').removeAttr('tabindex').end()
                    .removeAttr('aria-describedby role aria-hidden tabindex');
            }
        };
        Slick.prototype.buildArrows = function () {
            var _ = this;
            var newDefault = {
                prevArrow: 'Diaporama précédente',
                nextArrow: 'Diaporama suivante'
            };

            if (_.options.arrows === true ) {
                if (_.options.prevArrow === _.defaults.prevArrow) {
                    _.options.prevArrow = $(_.options.prevArrow).text(newDefault.prevArrow);
                    _.options.prevArrow = _.options.prevArrow[0].outerHTML;
                }
                if (_.options.nextArrow === _.defaults.nextArrow) {
                    _.options.nextArrow = $(_.options.nextArrow).text(newDefault.prevArrow);
                    _.options.nextArrow = _.options.nextArrow[0].outerHTML;
                }
            }

            // Slick default
            buildArrows.apply(this, arguments);

        };

        $el.slick("destroy");
        document.body.removeChild($el[0]);

    });

})(jQuery, Drupal, this, this.document);;
(function ($, Drupal, window, document, undefined) {

    // STICKY MEGA MENU
    function doStickyMegaMenu() {
        var identificationSite = $('.identification-site');
        if (identificationSite.length) {
            $('.mega-menu:not(.mega-menu-paps)').affix({
                offset: {
                    top: parseInt(identificationSite.offset().top) + parseInt(identificationSite.height())
                }
            });
        }
    }
    // MEGA MENU
    function doMegaMenu() {
        // jQuery Store
        var megaMenuBurgerResponsive = $('.mega-menu--burger-responsive');
        var menu = $('.mega-menu');
        var menuItemLink = $('.mega-menu--item--link');
        var mainMenu = $('#main-menu');

        // Init state for orientation change
        megaMenuBurgerResponsive.removeClass('mega-menu--burger-responsive--is-opened').attr('aria-expanded', 'false');
        $('.mega-menu--item.active, .mega-menu--layer--niv2--item.active').removeClass('active');
        menuItemLink.attr('aria-selected','false');
        $('.mega-menu--layer').attr('aria-hidden','true');
        $('.mega-menu--layer--niv2--tabpane.active').removeClass('active').attr('aria-hidden','true');


        // Manage clic on burger icon
        megaMenuBurgerResponsive.off('click').on('click', function() {
            if (menu.hasClass('mega-menu-opened')) {
                menu.removeClass('mega-menu-opened');
                $(this).removeClass('mega-menu--burger-responsive--is-opened');
                $('.overlay').removeClass('overlay-menu').hide();
            }
            else {
                menu.addClass('mega-menu-opened');
                $(this).addClass('mega-menu--burger-responsive--is-opened');
                $('.overlay').addClass('overlay-menu').show();
                var regionHeaderMenuItems = $('.region-header>li.mega-menu--item');
                if (regionHeaderMenuItems.hasClass('active')) {
                    regionHeaderMenuItems.removeClass('active');
                    $('.region-header>li.mega-menu--item>.mega-menu--item--link').attr('aria-selected','false');
                    $('.region-header>li.mega-menu--item>.mega-menu--layer').attr('aria-hidden','true');
                }
            }
        });
        // Manage click event on level 1
        menuItemLink.off('click').on('click', function() {
            // If menu item isn't selected yet
            if(!$(this).parents('.mega-menu--item').hasClass('active')) {
                // Display normal state for all items
                mainMenu.find('.mega-menu--item.active').removeClass('active');
                mainMenu.find('.mega-menu--item--link').attr('aria-selected','false');
                mainMenu.find('.mega-menu--layer').attr('aria-hidden','true');
                // Display active state for selected item
                $(this).parents('.mega-menu--item').addClass('active');
                $(this).attr('aria-selected','true');
                $('#'+$(this).attr('aria-controls')).attr('aria-hidden','false');
                // Set min height for level 3 (cosmetic)
                $('.mega-menu--layer--niv2--tabcontent').each(function() {
                    $(this).css('min-height',parseInt($(this).parents('.mega-menu--layer').find('.mega-menu--layer--niv2--list').height()) + 'px');
                });
                if (isMobileMenu && $(this).attr('id')=='tab_recherche') {
                    if (megaMenuBurgerResponsive.hasClass('mega-menu--burger-responsive--is-opened')) {
                        menu.removeClass('mega-menu-opened');
                        megaMenuBurgerResponsive.removeClass('mega-menu--burger-responsive--is-opened');
                        mainMenu.find('.mega-menu--item.active').removeClass('active');
                        mainMenu.find('.mega-menu--item--link').attr('aria-selected','false');
                        mainMenu.find('.mega-menu--layer').attr('aria-hidden','true');
                    }
                    $('.overlay').removeClass('overlay-menu');
                    $('.overlay').show();
                }
                if (!isMobileMenu && $(this).attr('id')=='tab_recherche') {
                    window.scrollTo(0,0);
                    $('#main-menu').css('position','absolute');
                }
                else {
                    $('#main-menu').removeAttr('style');
                }
            }
            else {
                if (isMobileMenu) {
                    $(this).parents('.mega-menu--item').removeClass('active');
                    $(this).attr('aria-selected','false');
                    $('#'+$(this).attr('aria-controls')).attr('aria-hidden','true');
                }
                if (isMobileMenu && $(this).attr('id')=='tab_recherche') {
                    $(this).attr('aria-selected','false');
                    $('#'+$(this).attr('aria-controls')).attr('aria-hidden','true');
                    $(this).parents('.mega-menu--item').removeClass('active');
                    $('.overlay').removeClass('overlay-menu');
                    $('.overlay').hide();
                }

                if (!isMobileMenu && $(this).attr('id')=='tab_recherche') {
                    $('#main-menu').removeAttr('style');
                }
            }
            return false;
        });
        // Manage click event on level 2
        var eventsLeve2 = (isMobileMenu)?'click':'mouseenter click';
        $('.mega-menu--layer--niv2--item>button').off().on(eventsLeve2,function(e) {
            e.preventDefault();
            // If layer isn't displayed yet
            if (!$($(this).attr("name")).hasClass('active')) {
                // Display normal state
                $('.mega-menu--layer--niv2--tabpane.active').removeClass('active').attr('aria-hidden','true');
                $('.mega-menu--layer--niv2--list .mega-menu--layer--niv2--item.active').removeClass('active').find('>a').attr('aria-selected','false');
                // Display active state for selected layer
                $($(this).attr("name")).addClass('active').attr('aria-hidden','false');
                $(this).attr('aria-selected','true').parent().addClass('active');
            }else{
                if (isMobileMenu) {
                    $($(this).attr("name")).removeClass('active').attr('aria-hidden','true');
                    $(this).attr('aria-selected','false').parent().removeClass('active');
                }
            }
        });

        // Manage click event outside Mega Menu
        $(document).on('click', function (evt) {
            if ((!isMobileMenu) && (!$(evt.target).is('.mega-menu--layer')) && (!$(evt.target).is('input, button')) && (!$(evt.target).parents('.mega-menu--layer').length)) {
                $('.mega-menu--layer--niv2--list .mega-menu--layer--niv2--item.active').removeClass('active').find('button').attr('aria-selected','false');
                $('.mega-menu--layer--niv2--list .mega-menu--layer--niv2--item:first-child').addClass('active').find('button').attr('aria-selected','true');
                $('.mega-menu--item.active').removeClass('active').attr('aria-selected','false');
                $('#main-menu').removeAttr('style');
            }
        });

        if (isMobileMenu) {
            menu.removeClass('mega-menu-opened');
            $('.mega-menu--layer--niv2--list .mega-menu--layer--niv2--item:first-child').removeClass('active').find('button').attr('aria-selected','false');
            if ($('.mega-menu--layer--niv2--tabcontent .mega-menu--layer--niv2--tabpane').length) {
                $('.mega-menu--layer--niv2--item>button').each(function() {
                    $(this).parent().append($('#' + $(this).attr('aria-controls')));
                });
            }
        }
        else {
            // Init state for orientation change
            if ($('.mega-menu--layer--niv2--list .mega-menu--layer--niv2--tabpane').length) {
                $('.mega-menu--layer--niv2--item').each(function() {
                    $(this).parents('.mega-menu--layer').find('.mega-menu--layer--niv2--tabcontent').append($('#' + $(this).find('>a').attr('aria-controls')));
                });
            }
            $('.mega-menu--layer--niv2--list .mega-menu--layer--niv2--item:first-child').addClass('active').find('a').attr('aria-selected','true');
            $('.mega-menu--layer--niv2--tabpane:first-child').addClass('active').attr('aria-hidden','false');
        }
    }
    // LINK TO TOP OF PAGE
    function doTopOfPage () {
        $(document).on("load scroll", function() {
            if ($(window).scrollTop() > 15) {
                $('.top-of-page').fadeIn();
            } else {
                $('.top-of-page').fadeOut();
            }
        });
    }
    // CHANGE SITE ON FOOTER SELECT
    function doFooterSelectSite () {
        $('.footer-select select').on('change', function(e) {
            e.preventDefault();
            if ($(this).val() != 0) {
                window.open($(this).val(), '_blank');
            }
        });
    }
    function doRegionSelect () {
        if ($('.accueil-choix-region--select select') && $('.accueil-choix-region--select select').length > 0) {
            $('.accueil-choix-region--select select').on('change', function (e) {
                e.preventDefault();
                if ($(this).val() != 0) {
                    window.open($(this).val(), '_blank');
                }
            });
        }
    }
    function hideButtonSuggerer(){
        $("button[name^='ief-add-submit']").hide();
        $("button[name^='ief-add-cancel']").hide();
    }
    // TELECOMMANDE (DESK + TABLETTE)
    function telecommande() {
        var telecommandeWidth = 207;
        $('.telecommande .mainBtn').css('width', telecommandeWidth + 'px');
        /* Desktop*/
        if ($(window).width()>1199){
            $('.telecommande ul>li').stop().css({width: '57px'});
            $(document).on('mouseover', '.telecommande', function () {
                $('.telecommande ul>li').stop().animate({width: telecommandeWidth + 'px'});
            });
            $(document).on('mouseout', '.telecommande', function () {
                $('.telecommande ul>li').stop().animate({width: '57px'});
            });
        }
        /* Tablette */
        if ($(window).width()>1023 && $(window).width()<1200){
            $('.telecommande ul>li').stop().css({width: 0});
            $(document).on('click', '.btn-tablette-telecommande', function () {
                $('.telecommande ul>li').stop().animate({width: telecommandeWidth + 'px'});
                $('.telecommande .btn-tablette-telecommande').addClass('close-telecommande');
            });

            $(document).on('click', function (evt) {
                var widthLi = $('.telecommande ul>li').width();
                if (!$(evt.target).closest('.telecommande').length && widthLi == telecommandeWidth){
                    $('.telecommande ul>li').stop().animate({width: 0});
                    $('.telecommande .btn-tablette-telecommande').removeClass('close-telecommande');
                }
            });
            $(document).on('click', '.close-telecommande', function (evt) {
                    $('.telecommande ul>li').stop().animate({width: 0});
                    $(this).removeClass('close-telecommande');
            });
        }

    }

    // Gestion des filtres des listes
    function filtresListes() {
        if ($('.page-liste--formulaire-wrapper').length) {
            $(document).on('click','.page-liste--formulaire-wrapper .btn-filtrer',function(e){
                e.preventDefault();
                $('body').css({overflow:'hidden'});
                $('body>.main-container>.row>section.col-sm-12').css({position:'static'});

                $('.page-liste--formulaire').show();

                $('.page-liste--filtres .form-actions .form-submit').on('click',function () {
                    if (isMobile) {
                        $('body').css({overflow:'auto'});
                    }
                });
            });
            $(document).on('click','.page-liste--formulaire .btn-close-filters',function(e){
                e.preventDefault();
                $('body').css({overflow:'auto'});
                $('body>.main-container>.row>section.col-sm-12').css({position:'relative'});
                $('.page-liste--formulaire').hide();
            });
            $('.page-liste--formulaire-wrapper button[data-toggle="collapse"]').removeAttr('aria-haspopup');
            $('.dropdown-menu').attr('aria-hidden','true');
            $('.page-liste--formulaire-wrapper .dropdown-menu.collapse').each(function () {
                $(this).on('hidden.bs.collapse', function () {
                    $('.page-liste--formulaire-wrapper .dropdown-menu.collapse.in').collapse('hide');
                    $(this).attr('aria-hidden','true');
                });
                $(this).on('shown.bs.collapse', function () {
                    $(this).attr('aria-hidden','false').removeAttr('aria-expanded');

                });
            });
            $('.page-liste--rappel--init').on('click', function (e) {
                e.preventDefault();
                $(".page-liste--formulaire-wrapper form .form-actions button[type='submit'] + button[type='submit']").submit();
            });

            $('.page-liste--formulaire-wrapper input:checkbox, .page-liste--formulaire-wrapper input:radio').on('change', function () {
                if (!isMobile) {
                    $(this).parents('form').submit();
                }
            });
            $(".page-liste--formulaire-wrapper button[type='submit']").on('click', function () {
                if (!isMobile) {
                    $(this).parents('form').submit();
                }
            });
            $('.page-liste--filtres .form-actions .form-submit').on('click',function () {
                if (isMobile) {
                    $('body').css({overflow:'auto'});
                }
            });
            //Hide category empty
            $('.page-liste--formulaire .form-select').each(function () {
                if ($(this).find('option').length === 0) {
                    $(this).hide();
                    $(this).parent('.js-form-item-themes-publications').hide();
                    /* Categorie Territoire */
                    $(this).parent('.form-item-field-territoires-ars-target-id').hide();
                }
            });
            $('.page-liste--formulaire > .form-item').each(function () {
                if ($(this).find('.fieldgroup.form-composite').find('.form-item').length === 0) {
                    $(this).hide();
                }
            });

            // Hide picto filtre if empty
            if($('.page-liste--formulaire .form-select option').length == 0 && $('.page-liste--formulaire > .form-item .fieldgroup.form-composite .form-item').length == 0){
                $('.page-liste--filtres').hide();
            }

            if($('#edit-field-date-publication-ars').val() || $('#edit-field-date-publication-ars-end').val()) {
                if($('#edit-field-date-publication-ars').val() != '' || $('#edit-field-date-publication-ars-end').val() != '') {
                    $('.page-liste--filtres').show().children().show();
                    $('.page-liste--formulaire > .form-item').each(function () {
                        if ($(this).find('.fieldgroup.form-composite').find('.form-item').length === 0) {
                            $(this).show();
                        }
                    });

                }
            }
        }
    }

    function sommaireWithStickyMenu () {
        "use strict";
        if(!isMobileMenu){
            if ($('.page-article--sommaire').length) {
                var hash = window.location.hash;
                hash = hash.substring(hash.indexOf('#'));
                if (hash) {
                    $('html, body').animate({scrollTop:$(hash).offset().top - $('#main-menu').height() + 'px'});
                }
                $('.page-article--sommaire ul li a').off('click').on('click', function (e) {
                    e.preventDefault();
                    if ($($(this).attr('href')).length) {
                        var offsetTop = $($(this).attr('href')).offset().top;
                        setTimeout(function () {
                            $('html, body').animate({scrollTop:offsetTop - $('#main-menu').height() + 'px'});
                        },100);
                    }


                });
            }
        }
    }
    // TEMPORAIRE :: A virer lorsque les modules seront prêts
    function carrouselSlick() {
        var settings;
        if ($('body:not(.body__ars_theme--arstheme_paps) .accueil-partenaires').length) {
            var accueilPartenairesList = $('.accueil-partenaires--list');
            accueilPartenairesList.on('init', function(){
                $('.accueil-partenaires .slick-prev').text('Partenaire précédent');
                $('.accueil-partenaires .slick-next').text('Partenaire suivant');
            });
            settings = {
                accessibility: true,
                slidesToShow: 4,
                slidesToScroll: 1,
                arrows: true,
                dots: false,
                responsive: [
                    {
                        breakpoint: 991,
                        settings: {
                            arrows: true,
                            slidesToShow: 2,
                            slidesToScroll: 2
                        }
                    },
                    {
                        breakpoint: 767,
                        settings: {
                            arrows: true,
                            slidesToShow: 1,
                            slidesToScroll: 1
                        }
                    }
                ]
            };
            accueilPartenairesList.slick(settings);
        }
        if ($('.accueil-services').length) {
            var accueilServicesListe = $('.accueil-services--liste');
            accueilServicesListe.on('init', function(){
                $('.accueil-services .slick-prev').text('Service précédent');
                $('.accueil-services .slick-next').text('Service suivant');
                $('.accueil-services .slick-arrow').removeAttr('aria-label');
                $('.accueil-services .slick-dots button').each(function(i) {
                    $(this).text('Actualité ' + $(this).text());
                });
            });
            settings = {
                accessibility: true,
                slidesToShow: 3,
                slidesToScroll: 3,
                arrows: true,
                dots: false,
                responsive: [
                    {
                        breakpoint: 1199,
                        settings: {
                            slidesToShow: 3,
                            slidesToScroll: 3,
                            dots: true
                        }
                    },
                    {
                        breakpoint: 991,
                        settings: {
                            slidesToShow: 2,
                            slidesToScroll: 2,
                            dots: true
                        }
                    },
                    {
                        breakpoint: 767,
                        settings: {
                            slidesToShow: 1,
                            slidesToScroll: 1,
                            dots: true
                        }
                    }
                ]
            };
            accueilServicesListe.slick(settings);
        }
        var $sliderActu = document.getElementById('slider-actus');
        if($sliderActu) {
            if ($('.accueil-carrousel-alternative').length) {
                if ($('.accueil-carrousel-actus--item').length < 2) {
                    $('.accueil-carrousel-alternative .carrousel-pause').hide();
                }

                var accueilCarrouselActusList = $('.accueil-carrousel-actus--list>div');
                accueilCarrouselActusList.on('init', function(){
                    $('.accueil-carrousel-actus--list .slick-prev').text('Actualité précédente');
                    $('.accueil-carrousel-actus--list .slick-next').text('Actualité suivante');
                    $('.accueil-carrousel-actus--list .slick-dots button').each(function(i) {
                        $(this).text('Actualité ' + $(this).text());
                    });
                });
                settings = {
                    accessibility: true,
                    slidesToShow: 1,
                    slidesToScroll: 1,
                    autoplay:true,
                    arrows: true,
                    dots: true,
                    pauseOnHover: false
                };
                accueilCarrouselActusList.slick(settings);

                $(document).on('click','.accueil-carrousel-alternative .carrousel-pause', function() {
                    if(!$(this).hasClass('play')) {
                        $(this).addClass('play');
                        $('.accueil-carrousel-actus--list>div').slick('slickPause');
                    }
                    else {
                        $(this).removeClass('play');
                        $('.accueil-carrousel-actus--list>div').slick('slickPlay');
                    }
                });
            }
        }
        var $slider = document.getElementById('slider-simple');
        if($slider){
            if ($('.accueil-carrousel-alternative').length) {
                var accueilCarrouselAlternaticeList = $('.accueil-carrousel-alternative--list>div');
                accueilCarrouselAlternaticeList.on('init', function(){
                    $('.accueil-carrousel-alternative--list .slick-prev').text('Actualité précédente');
                    $('.accueil-carrousel-alternative--list .slick-next').text('Actualité suivante');
                });
                settings = {
                    accessibility: true,
                    slidesToShow: 1,
                    slidesToScroll: 1,
                    autoplay:true,
                    arrows: false,
                    dots: false,
                    pauseOnHover: false
                };
                accueilCarrouselAlternaticeList.slick(settings);

                $(document).on('click','.accueil-carrousel-alternative .carrousel-pause-thematique', function() {
                    if(!$(this).hasClass('play')) {
                        $(this).addClass('play');
                        $('.accueil-carrousel-alternative--list>div').slick('slickPause');
                    }
                    else {
                        $(this).removeClass('play');
                        $('.accueil-carrousel-alternative--list>div').slick('slickPlay');
                    }
                });
            }
        }
    }

    //Fonction recherche en RWD
    function doSearchButtonRwd() {
        var megaMenuInnerTabRecherche = $('.mega-menu--inner .mega-menu--item[role="search"]');
        var regionHeaderList = $('.region-header>.mega-menu--item[role="search"]');
        if (isMobileMenu && megaMenuInnerTabRecherche.length) {
            $('.region-header').append(megaMenuInnerTabRecherche);
        }
        if (!isMobileMenu && regionHeaderList.length) {
            $('.mega-menu--inner .mega-menu--list').append(regionHeaderList);
        }
    }
    // Fonction carrousel etude rapport
    function doEtudePublicationSlider() {
        var rapportShowSlider = $('.page-etude-rapport--slide-show--list');
        if (rapportShowSlider.length && !(rapportShowSlider.hasClass('slick-initialized'))) {
            var settings = {
                accessibility: true,
                slidesToShow: 1,
                slidesToScroll: 1,
                arrows: true,
                dots: false
            };

            rapportShowSlider.on('init reInit afterChange', function (event, slick, currentSlide) {
                var i = (currentSlide ? currentSlide : 0) + 1;
                var pagination = $('.page-etude-rapport--slide-show .pagination');
                pagination.find('span.index').text(i);
                pagination.find('span.total').text(slick.slideCount);
                $('.page-etude-rapport--slide-show .slick-prev').text('Document précédent');
                $('.page-etude-rapport--slide-show .slick-next').text('Document suivant');
            }).slick(settings);
        }
    }

    // Fonction carrousel article
    function doArticleSlider() {
        var pageArticleSlideShowList = $('.page-article--slide-show--list');
        if (pageArticleSlideShowList.length) {
            $('.accueil-carrousel-alternative--list>div').on('init', function(){
                $('.page-article--slide-show--list .slick-prev').text('Image précédente');
                $('.page-article--slide-show--list .slick-next').text('Image suivante');
                $('.page-article--slide-show--list .slick-dots button').each(function(i) {
                    $(this).text('Image ' + $(this).text());
                });
            });
            var settings = {
                accessibility: true,
                slidesToShow: 1,
                slidesToScroll: 1,
                arrows: true,
                dots: false,
                responsive: [
                    {
                        breakpoint: 767,
                        settings: {
                            arrows: true,
                            dots: true,
                            slidesToShow: 1,
                            slidesToScroll: 1
                        }
                    }
                ]
            };
            pageArticleSlideShowList.slick(settings);
        }
        /* Resize window to reinit slick dimensions in collapse content article */
        if($('.page-article--collapse-btn').length){
            $('.page-article--collapse-btn.collapsed').off().on('click', function(){
                setTimeout(function () {
                    if($('.page-article--slide-show').length) {
                        $(window).trigger('resize');
                    }
                },10);
            });
        }
    }

